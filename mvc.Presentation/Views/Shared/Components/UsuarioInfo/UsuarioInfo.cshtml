@model SR.Entities.BaseEntities.UsuarioEntities.Usuario
<div>
    <div class="heading-03-bold color-texto ">@Model.Titulo</div>
</div>
<div class="d-flex justify-content-between align-items-center p-3  rounded mb-3">
  
    
    <div class="d-flex align-items-center">
        <i class="bi bi-person-circle text-primary fs-3 me-2"></i>
        <div>
            <div class="body-regular-01-texto table-background">@Model.Nombre</div>
        </div>
        <a asp-controller="Login" asp-action="Logout" title="Salir" class="text-danger">
            <i class="bi bi-box-arrow-in-right fs-1" ></i>
        </a>
    </div>
  
</div>

